<img
     src="{{ get_image(imgID).src(imgSize ~ '-desktop') }}"
     data-srcset="{{ get_image(imgID).src(imgSize ~ '-mobile') }} 360w,
     {{ get_image(imgID).src(imgSize ~ '-tablet') }} 768w,
     {{ get_image(imgID).src(imgSize ~ '-desktop') }} 1440w,
     {{ get_image(imgID).src(imgSize ~ '-jumbo') }} 2000w"
     width="{{ get_image(imgID).width }}"
     height="{{ get_image(imgID).height }}"
     alt="{{get_image(imgID).alt}}"
/>
